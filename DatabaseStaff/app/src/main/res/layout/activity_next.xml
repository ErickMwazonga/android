<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.erickmwazonga.databasestaff.NextActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Load the username"
        android:id="@+id/textView" />

    <EditText
        android:layout_width="256dp"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:id="@+id/editTextLUsername" />

    <Button
        android:layout_width="255dp"
        android:layout_height="wrap_content"
        android:text="LOAD INTERNAL CACHE"
        android:layout_gravity="center"
        android:id="@+id/buttonLInternalCache" />

    <Button
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:text="LOAD EXTERNAL CACHE"
        android:layout_gravity="center"
        android:id="@+id/buttonLExternalCache" />

    <Button
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:text="LOAD EXTERNAL PRIVATE"
        android:layout_gravity="center"
        android:id="@+id/buttonLExternalPrivate" />

    <Button
        android:layout_width="254dp"
        android:layout_height="wrap_content"
        android:text="LOAD EXTERNAL PUBLIC"
        android:layout_gravity="center"
        android:id="@+id/buttonLExternalPublic" />

    <Button
        android:layout_width="256dp"
        android:layout_height="wrap_content"
        android:text="PREVIUS"
        android:layout_gravity="center"
        android:id="@+id/buttonPrevius" />

</LinearLayout>
